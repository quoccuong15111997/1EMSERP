<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_margin="10dp"    app:cardCornerRadius="5dp"    android:elevation="5dp"    android:layout_height="wrap_content">    <androidx.constraintlayout.widget.ConstraintLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:padding="10dp">        <LinearLayout            android:id="@+id/linearLayout"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginTop="8dp"            android:orientation="horizontal"            android:paddingLeft="10dp"            android:paddingRight="10dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent">            <LinearLayout                android:id="@+id/linearLayout6"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:layout_marginRight="3dp"                android:layout_weight="1"                android:orientation="vertical"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent">                <TextView                    android:id="@+id/txtTitleDateBegin"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:text="Từ ngày"                    android:textAppearance="@android:style/TextAppearance.Material.Small"                    android:textSize="14sp" />                <com.firstems.erp.helper.widgets.EMSConstrainLayoutBorder                    android:id="@+id/layoutDateForm"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_weight="1"                    android:background="@color/text__white">                    <com.firstems.erp.helper.widgets.EMSTextviewHighLineNonBorder                        android:id="@+id/txtDateBegin"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:background="@color/text__white"                        android:text="15/05/2020"                        android:textAlignment="center"                        android:textColor="@color/text_color"                        app:layout_constraintBottom_toBottomOf="parent"                        app:layout_constraintStart_toStartOf="parent"                        app:layout_constraintTop_toTopOf="parent" />                    <ImageView                        android:id="@+id/imageView24"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        app:layout_constraintBottom_toBottomOf="@+id/txtDateBegin"                        app:layout_constraintEnd_toEndOf="parent"                        app:layout_constraintHorizontal_bias="1.0"                        app:layout_constraintStart_toEndOf="@+id/txtDateBegin"                        app:layout_constraintTop_toTopOf="@+id/txtDateBegin"                        app:srcCompat="@drawable/ic_calenda" />                </com.firstems.erp.helper.widgets.EMSConstrainLayoutBorder>            </LinearLayout>            <LinearLayout                android:id="@+id/linearLayout7"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="3dp"                android:layout_marginTop="5dp"                android:layout_weight="1"                android:orientation="vertical"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent">                <TextView                    android:id="@+id/txtTitleDateEnd"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:text="Đến ngày"                    android:textAppearance="@android:style/TextAppearance.Material.Small"                    android:textSize="14sp" />                <com.firstems.erp.helper.widgets.EMSConstrainLayoutBorder                    android:id="@+id/layoutDateEnd"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_weight="1"                    android:background="@color/text__white">                    <com.firstems.erp.helper.widgets.EMSTextviewHighLineNonBorder                        android:id="@+id/txtDateEnd"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:background="@color/text__white"                        android:text="15/05/2020"                        android:textAlignment="center"                        android:textColor="@color/text_color"                        app:layout_constraintBottom_toBottomOf="parent"                        app:layout_constraintStart_toStartOf="parent"                        app:layout_constraintTop_toTopOf="parent" />                    <ImageView                        android:id="@+id/imageView242"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        app:layout_constraintBottom_toBottomOf="@+id/txtDateFrom"                        app:layout_constraintEnd_toEndOf="parent"                        app:layout_constraintHorizontal_bias="1.0"                        app:layout_constraintStart_toEndOf="@+id/txtDateFrom"                        app:layout_constraintTop_toTopOf="@+id/txtDateFrom"                        app:srcCompat="@drawable/ic_calenda" />                </com.firstems.erp.helper.widgets.EMSConstrainLayoutBorder>            </LinearLayout>        </LinearLayout>        <TextView            android:id="@+id/txtTitleStatus"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginTop="16dp"            android:background="@drawable/custom_background_item"            android:padding="10dp"            android:text="Trạng thái"            android:textColor="@color/text_color"            android:textStyle="bold"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/linearLayout" />        <CheckBox            android:id="@+id/chkChuaTrinhKy"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="8dp"            android:layout_marginTop="5dp"            android:buttonTint="@color/colorAccent"            android:text="Chưa trình ký"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.0"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/txtTitleStatus" />        <View            android:id="@+id/view4"            android:layout_width="0dp"            android:layout_height="1dp"            android:background="@color/colorGray"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/chkChuaTrinhKy" />        <CheckBox            android:id="@+id/chkChoDuyet"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            android:buttonTint="@color/colorAccent"            android:text="Chờ duyệt"            app:layout_constraintStart_toStartOf="@+id/chkChuaTrinhKy"            app:layout_constraintTop_toBottomOf="@+id/view4" />        <View            android:id="@+id/view5"            android:layout_width="0dp"            android:layout_height="1dp"            android:background="@color/colorGray"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/chkChoDuyet" />        <CheckBox            android:id="@+id/chkHoanTat"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            android:buttonTint="@color/colorAccent"            android:text="Hoàn tất"            app:layout_constraintStart_toStartOf="@+id/chkChoDuyet"            app:layout_constraintTop_toBottomOf="@+id/view5" />        <View            android:id="@+id/view6"            android:layout_width="0dp"            android:layout_height="1dp"            android:background="@color/colorGray"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/chkHoanTat" />        <com.balysv.materialripple.MaterialRippleLayout            style="@style/RippleStyleWhite"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_marginTop="8dp"            android:layout_marginBottom="8dp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/view6">            <Button                android:id="@+id/btnSort"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:layout_gravity="center"                android:background="@drawable/bg_rounded_btn_scan_product"                android:paddingLeft="@dimen/margin65"                android:paddingRight="@dimen/margin65"                android:text="Lọc trình ký"                android:textAllCaps="false"                android:textColor="@color/text__white"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/layoutGraphsearch" />        </com.balysv.materialripple.MaterialRippleLayout>    </androidx.constraintlayout.widget.ConstraintLayout></androidx.cardview.widget.CardView>