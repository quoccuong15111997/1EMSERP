<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    xmlns:app="http://schemas.android.com/apk/res-auto">    <androidx.cardview.widget.CardView        android:id="@+id/cardView2"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_margin="5dp"        app:cardCornerRadius="@dimen/radius_card_medium"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <LinearLayout            android:id="@+id/lAssets"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="horizontal"            android:padding="10dp"            android:background="@color/colorAccent">            <TextView                android:id="@+id/txtLocatioName"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_weight="2"                android:text="ssssss"                android:layout_gravity="center"                android:textColor="@color/text__white"                android:textSize="16dp"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent" />        </LinearLayout>    </androidx.cardview.widget.CardView></androidx.constraintlayout.widget.ConstraintLayout>